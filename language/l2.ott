   {{ lem EnvEmp }}
   {{ ocaml assert false }}
 
 E_k {{ tex \ottnt{E}^{\textsc{k} } }} :: 'E_k_' ::=             {{ phantom }}
                                                                 {{ hol (id-> k) }}
