     {{ lem (List.fold_right (fun (x,v) m -> Pmap.add x v m) [[id1 k1 .. idn kn]] Pmap.empty) }} 
   | E_k1 u+ .. u+ E_kn                                          :: M :: union
     {{ hol (FOLDR FUNION FEMPTY [[E_k1..E_kn]]) }}
     {{ lem (List.fold_right union_map [[E_k1..E_kn]] Pmap.empty) }}
     {{ ocaml (assert false) }}
 
